{
	"name": "react-pokemons-app",
	"version": "1.0.0",
	"description": "An awesome application to handle some pokemons.",
	"dependencies": {
		"@types/node": "22.14.1",
		"@types/react": "19.1.2",
		"@types/react-dom": "19.1.2",
		"@types/react-router-dom": "^5.3.3",
		"json-server": "^1.0.0-beta.3",
		"prettier": "^3.5.3",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-router-dom": "^7.5.1",
		"react-scripts": "^5.0.1",
		"typescript": "4.9.5"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"format": "prettier --write \"**/*.{js,ts,tsx, json, html, css}\""
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"cross-env": "^7.0.3",
		"husky": "^9.1.7",
		"lint-staged": "^15.5.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.ts": [
			"prettier --write",
			"git add"
		],
		"*.tsx": [
			"prettier --write",
			"git add"
		]
	}
}
